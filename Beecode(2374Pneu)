#include <iostream>
using namespace std;

int main() {

    int n;
    int m;
    int re;
    
    cin >> n;
    cin >> m;
    
    if (m >= 1 && m <= 40){
        if(n >= 1 && n <= 40){
            re = n - m;
            cout << re << endl;
            
        }
    }
}
