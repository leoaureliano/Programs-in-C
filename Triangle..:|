#include <iostream>
using namespace std;

int a;
string b;


int main()
{
    cout << endl;
    for (int i = 1; i < 5; i++) {
        for (int l = 2; l > (i - 2); l--)
            cout << "-";
        for (int j = 5; j > (5-i); j--)
            cout << "*";
        
        cout << endl;
    }
    return 0;
}
